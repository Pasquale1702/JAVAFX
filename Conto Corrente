package application;

public class ContoCorrente {
    
    String pin;
    
    double saldo;
    
    
    public ContoCorrente(String pin, double saldo) {
    	
        this.pin = pin;
        
        this.saldo = saldo;
    }
    
    public String getPin() {
    	
        return pin;
    }
    
    public double getSaldo() {
    	
        return saldo;
    }
}
